{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "space-before-function-paren": 0,
    "import/no-unresolved": 0,
    "no-confusing-arrow": 0,
    "no-underscore-dangle": 0,
    "arrow-parens": 0,
    "keyword-spacing": [
      "error"
    ],
    "global-require": 0,
    "func-names": 0,
    "no-shadow": 0,
    "new-cap": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0
  },
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
